ctrl + cmd + alt - h : yabai -m window --focus west
ctrl + cmd + alt - j : yabai -m window --focus south
ctrl + cmd + alt - k : yabai -m window --focus north
ctrl + cmd + alt - l : yabai -m window --focus east

ctrl + cmd + alt + shift - h : yabai -m window --swap west
ctrl + cmd + alt + shift - j : yabai -m window --swap south
ctrl + cmd + alt + shift - k : yabai -m window --swap north
ctrl + cmd + alt + shift - l : yabai -m window --swap east

ctrl + cmd + alt - 1 : yabai -m space --focus 1
ctrl + cmd + alt - 2 : yabai -m space --focus 2
ctrl + cmd + alt - 3 : yabai -m space --focus 3
ctrl + cmd + alt - 4 : yabai -m space --focus 4
ctrl + cmd + alt - 5 : yabai -m space --focus 5
ctrl + cmd + alt - 6 : yabai -m space --focus 6

ctrl + cmd + shift - 1 : yabai -m window --space  1; yabai -m space --focus 1
ctrl + cmd + shift - 2 : yabai -m window --space  2; yabai -m space --focus 2
ctrl + cmd + shift - 3 : yabai -m window --space  3; yabai -m space --focus 3
ctrl + cmd + shift - 4 : yabai -m window --space  4; yabai -m space --focus 4
ctrl + cmd + shift - 5 : yabai -m window --space  5; yabai -m space --focus 5
ctrl + cmd + shift - 6 : yabai -m window --space  6; yabai -m space --focus 6

ctrl + cmd + alt - 0  : yabai -m display --focus 1
ctrl + cmd + alt - 9  : yabai -m display --focus 2
ctrl + cmd + alt - 8  : yabai -m display --focus 3

ctrl + cmd + alt + shift - 0  : yabai -m window --display 1; yabai -m display --focus 1
ctrl + cmd + alt + shift - 9  : yabai -m window --display 2; yabai -m display --focus 2
ctrl + cmd + alt + shift - 8  : yabai -m window --display 2; yabai -m display --focus 2

ctrl + cmd + alt - a : yabai -m window --move rel:-20:0
ctrl + cmd + alt - s : yabai -m window --move rel:0:20
ctrl + cmd + alt - w : yabai -m window --move rel:0:-20
ctrl + cmd + alt - d : yabai -m window --move rel:20:0

ctrl + cmd + alt - r : yabai -m space --rotate 90
ctrl + cmd + alt - return : yabai -m space --balance

ctrl + cmd + alt + shift - a : yabai -m window --resize left:-40:0
ctrl + cmd + alt + shift - s : yabai -m window --resize bottom:0:40
ctrl + cmd + alt + shift - w : yabai -m window --resize top:0:-40
ctrl + cmd + alt + shift - d : yabai -m window --resize right:40:0

ctrl + cmd + alt - t : yabai -m window --toggle float;\
         yabai -m window --grid 4:4:1:1:2:2

shift - up     : yabai -m window --grid 1:1:0:0:1:1
shift - left   : yabai -m window --grid 1:2:0:0:1:1
shift - right  : yabai -m window --grid 1:2:1:0:1:1
